% codigo generado automaticamente por la app de matlab

function [fitresult, gof] = createFits(tiempo, Posz, Posx, Posy)
%CREATEFITS(TIEMPO,POSZ,POSX,POSY)
%  Create fits.
%
%  Data for 'Posz' fit:
%      X Input : tiempo
%      Y Output: Posz
%  Data for 'Posx' fit:
%      X Input : tiempo
%      Y Output: Posx
%  Data for 'Posy' fit:
%      X Input : tiempo
%      Y Output: Posy
%  Output:
%      fitresult : a cell-array of fit objects representing the fits.
%      gof : structure array with goodness-of fit info.
%
%  See also FIT, CFIT, SFIT.

%  Auto-generated by MATLAB on 30-Aug-2022 23:47:38

%% Initialization.

% Initialize arrays to store fits and goodness-of-fit.
fitresult = cell( 3, 1 );
gof = struct( 'sse', cell( 3, 1 ), ...
    'rsquare', [], 'dfe', [], 'adjrsquare', [], 'rmse', [] );

%% Fit: 'Posz'.
[xData, yData] = prepareCurveData( tiempo, Posz );

% Set up fittype and options.
ft = fittype( 'poly2' );

% Fit model to data.
[fitresult{1}, gof(1)] = fit( xData, yData, ft );

% Plot fit with data.
figure( 'Name', 'Posz' );
h = plot( fitresult{1}, xData, yData );
legend( h, 'Posz vs. tiempo', 'Posz', 'Location', 'NorthEast', 'Interpreter', 'none' );
% Label axes
xlabel( 'tiempo', 'Interpreter', 'none' );
ylabel( 'Posz', 'Interpreter', 'none' );
grid on

%% Fit: 'Posx'.
[xData, yData] = prepareCurveData( tiempo, Posx );

% Set up fittype and options.
ft = fittype( 'poly1' );

% Fit model to data.
[fitresult{2}, gof(2)] = fit( xData, yData, ft );

% Plot fit with data.
figure( 'Name', 'Posx' );
h = plot( fitresult{2}, xData, yData );
legend( h, 'Posx vs. tiempo', 'Posx', 'Location', 'NorthEast', 'Interpreter', 'none' );
% Label axes
xlabel( 'tiempo', 'Interpreter', 'none' );
ylabel( 'Posx', 'Interpreter', 'none' );
grid on

%% Fit: 'Posy'.
[xData, yData] = prepareCurveData( tiempo, Posy );

% Set up fittype and options.
ft = fittype( 'poly1' );

% Fit model to data.
[fitresult{3}, gof(3)] = fit( xData, yData, ft );

% Plot fit with data.
figure( 'Name', 'Posy' );
h = plot( fitresult{3}, xData, yData );
legend( h, 'Posy vs. tiempo', 'Posy', 'Location', 'NorthEast', 'Interpreter', 'none' );
% Label axes
xlabel( 'tiempo', 'Interpreter', 'none' );
ylabel( 'Posy', 'Interpreter', 'none' );
grid on
